# shell alias
alias bs="source ~/.bashrc"

# tmux aliasses
alias tn="tmux new -s"
alias ta="tmux attach -t"
alias tl="tmux list-sessions"
alias tk="tmux kill-session -t"

# windows aliasses
alias notes="/mnt/c/Users/bram/AppData/Local/Programs/Mark\ Text/Mark\ Text.exe /home/bram/notes"

# git commands
alias gg="git log --all --decorate --oneline --graph"

# ranger
alias rn='ranger --choosedir=$HOME/.rangerdir; LASTDIR=`cat $HOME/.rangerdir`; cd "$LASTDIR"'
